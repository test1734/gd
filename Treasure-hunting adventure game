def treasure_story(): 
    print("  TREASURE QUEST ") 
    print("Our hero, Arin the Explorer, sets sail to the Lost Island of Zantara.") 
    print("His goal: to find the legendary Emerald Crown hidden deep in ancient 
ruins.") 
